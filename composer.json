{
  "name": "refinery29/solr-annotations",
  "description": "Annotations for Solr",
  "license": "MIT",
  "authors": [
    {
      "name": "Kayla Daniels",
      "email": "kayladnls@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^5.5 || ^7.0",
    "beberlei/assert": "^2.5.1",
    "doctrine/annotations": "^1.2.7"
  },
  "require-dev": {
    "codeclimate/php-test-reporter": "0.3.2",
    "phpunit/phpunit": "^4.8.26",
    "refinery29/php-cs-fixer-config": "0.4.19",
    "refinery29/test-util": "0.4.3"
  },
  "autoload": {
    "psr-4": {
      "Refinery29\\SolrAnnotations\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Refinery29\\SolrAnnotations\\Test\\": "test/"
    }
  }
}
